unsigned int ADC_average(void)
{
    signed char samples = 100;
    unsigned long value = 0;   
    while(samples > 0)
    {
        value += ((unsigned long)GetAdcValue());
        samples--;
    }  
    value /= 100; 
    return ((unsigned int)value);
}
